# OG
define normalizeOGTags      [ 0 <- "+OG" ]
	    		.o. [ 0 <- "+Old" ]
	    		.o. [ 0 <- "+Dialect" ]
	    		.o. [ 0 <- "+VPart" ] # Lela does not like this
			.o. [ უ <- u ]
			.o. [ ვ <- v ]
			.o. [ ე <- e ]
			.o. ["+Impv-I" <- "+ImpvPres"]
			.o. ["+Impv-II" <- "+ImpvAor"]
			.o. ["+Conj-I" <- "+Conj"]
			.o. ["+Conj-II" <- "+Opt"]
			.o. ["+Conj-III" <- "+ConjPerf"]
			.o. ["+Iter-II" <- "+IterAor"]
			.o. ["+IterPerf" <- "+IterPerf1"]
	    		.o. ["+Iter-III" <- "+IterPerf"]
			 # .o. [ 0 <- "+[Incl]" ]
			 # there are no reduced forms in OG?
			.o. [ \"+Att"* ]
	    		# .o. [ 0 <- "+Full" ]
	    		#.o. [ 0 <- "+Long", 0 <- "+DLong", 0 <- "+DDLong" ]
			.o. [ 0 <- "+DN", 0 <- "+DA", 0 <- "+DDN", 0 <- "+DDA"  ]
			# relatives
			.o. [ "+Rel:ცა" <- "+Encl:ცა" "+Foc" || "+Rel" ?* _ ]
 			.o. [ "+Rel" <- "+Int" 
			    || ["+Pron" | "+Adv" ] _ ?* ["+Encl:ცა" "+Foc" |"+RelProx"|"+RelMed"|"+RelDist"] ]
			.o. [ "+Rel" (<-) "+Int"
			    || "+Pron" _ ?* "+Gen" ]
	    		# (D)DDir/(D)DBen do not exist (??)
			.o. [ \["+DDir"|"+DBen"|"+DDDir"|"+DDBen"]* ]
			.o. [ \"+Aux"* ]
	    		.o. [ "+Obj1Sg" <- "+Obj1SgSh", # restore
			      "+Obj1Pl" <- "+Obj1PlSh",
			      # "+Obj1Pl" "+[Excl]" <- "+Obj1PlEx",
		  	      "+Obj2Sg" <- "+Obj2SgSh",
			      "+Obj2Pl" <- "+Obj2PlSh" ]
	    		.o. [ "+Obj1" <- "+Obj1Sg",
			      "+Obj1" <- "+Obj1Pl",
		  	      "+Obj2" <- "+Obj2Sg",
			      "+Obj2" <- "+Obj2Pl" ]
	    		.o. [ "+Obj1SgSh" <- "+Obj1Sg", # shadow these to protect them from rule above
			      "+Obj1PlSh" <- "+Obj1Pl",
		  	      "+Obj2SgSh" <- "+Obj2Sg",
			      "+Obj2PlSh" <- "+Obj2Pl"
		  	      || isInverted ?* _ ]
	    		.o. [ "+Subj1" <- "+Subj1Sg",
			      "+Subj1" <- "+Subj1Pl",
		  	      "+Subj2" <- "+Subj2Sg",
			      "+Subj2" <- "+Subj2Pl"
		  	      || isInverted ?* _ ]
	    		.o. [ "+ObjBen1" <- "+ObjBen1Sg", # Obj number is not marked for 1./2. pers, except გვ-
			      "+ObjLoc1" <- "+ObjLoc1Sg",
			      "+ObjTh1" <- "+ObjTh1Sg",
			      "+ObjBen1" <- "+ObjBen1Pl",
			      "+ObjLoc1" <- "+ObjLoc1Pl",
			      "+ObjTh1" <- "+ObjTh1Pl",
			      "+ObjBen2" <- "+ObjBen2Sg",
			      "+ObjLoc2" <- "+ObjLoc2Sg",
			      "+ObjTh2" <- "+ObjTh2Sg",
			      "+ObjBen2" <- "+ObjBen2Pl",
			      "+ObjLoc2" <- "+ObjLoc2Pl",
			      "+ObjTh2" <- "+ObjTh2Pl" ]
			.o. [ \"+NG"* ] # remove New Georgian only
	    		.o. [ 0       <- "+Imperfective", 
			      "+Pres" <- "+Fut",
			      "+Conj" <- "+ConjPres",
			      "+Conj" <- "+ConjFut",
			      "+Impf" <- "+Cond" ]
			# .o. [ უ <- ვ || \Vowel _ [ა|ე|ო] ]
			.o. [ ჱ <- {ეჲ} ]
			.o. [ ჳ <- {ვი} || ? _ ]
			.o. [ "{""ი""}" <- {I} ]
			.o. [ 0	<- {][} ]
			.o. [ {[ე]} <- {E} ]
			.o. [ {[ა]} <- {A} ]
			.o. [ {[ვ]} <- {V} ]
			# .o. AtoAdv
			;



