endif

define OGIndSpeech     [ 0:["–"|"-"]
       		         [ "+Encl:IndSp1":{მეთქი}
		       	 | "+Encl:IndSp2":{თქუა} ]
		       | (0:["–"|"-"]) "+Encl:IndSp3":{ო} ] ;

define OGNounEnclitics  [ ( 0:["–"|"-"] )
       			  [ "+Encl:მე":{მე}
       			  | "+Encl:ცა":{ცა} "+Foc":0
			  | "+Encl:ღა":{ღა} "+Foc":0
			  | "+Encl:ვე":{ვე} "+Foc":0
			  | "+Encl:მცა":{მცა} "+Foc":0
			  | "+Encl:ძი":{ძი}
			  ] ]*
			( OGIndSpeech | "+Encl:Q":{-ა} ) ;

define OGVerbEnclitics  [ ( 0:["–"|"-"] )
       			  [ "+Encl:ცა":{ცა} "+Foc":0
			  | "+Encl:ღა":{ღა} "+Foc":0
			  | "+Encl:მცა":{მცა} "+Foc":0
			  | "+Encl:ძი":{ძი}
			  | "+Encl:რე":{რე}
			  ] ]*
			( OGIndSpeech ) ;

define OGTmesisInfix      [ {მე} "+Tm:მე":0
