define oldGeoOrth	    [ {ეჲ}  (->) ჱ ]
       			.o. [ {ვი}  (->) ჳ ]
			.o. [  ვ    (->) უ || _ [ა|ე|ო] ]
			.o. [ V -> ვ ]
			.o. [ {უ} (->) {ოჳ} ]
			.o. [ ხ (->) ჴ ] # ??
			.o. [ {უა} (->) {უვა} ]
			.o. [ {ოო} (->) {ოვო} ]
